KUBE_VERSION="1.21.1"
# NW_PLUGIN can be either calico or weave
NW_PLUGIN="calico"
INSECURE_DASHBOARD="no"
LB_IPPOOL="172.16.26.200-172.16.26.250"
INSTALL_HELM="yes"
ENABLE_OLM="yes"