BOX_IMAGE="coolsvap/k8s-node"
BOX_VERSION="1.21.3"
PROVIDER="virtualbox"

SETUP_MASTER=true
MASTER_IP="172.16.26.10"
POD_NW_CIDR="192.168.0.0/16"
MASTER_HOSTNAME="k8s-master"
MASTER_MEMORY="4096"
MASTER_CPU="2"

SETUP_NODES=true
NODE_COUNT="1"
NODE_IP_NW="172.16.26."
